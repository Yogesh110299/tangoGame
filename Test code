<div id="test-output" style="margin: 20px; font-size: 18px; color: green;">
  Waiting for script...
</div>

<script type="text/javascript">
  function initMyTest() {
    alert("ğŸš€ initMyTest() called");

    class TangoGame {
      constructor() {
        alert("âœ… TangoGame instantiated");
        const el = document.getElementById('test-output');
        if (el) {
          el.textContent = "âœ¨ TangoGame class has successfully loaded!";
        } else {
          alert("âš ï¸ 'test-output' div not found!");
        }
      }
    }

    new TangoGame();
  }

  // Safe DOM Ready detection for SharePoint Modern
  if (document.readyState === 'complete' || document.readyState === 'interactive') {
    alert("ğŸ“¦ DOM ready â€” running initMyTest()");
    initMyTest();
  } else {
    alert("ğŸ“¦ Waiting for DOMContentLoaded");
    document.addEventListener('DOMContentLoaded', function () {
      alert("ğŸ“¦ DOMContentLoaded fired â€” calling initMyTest()");
      initMyTest();
    });
  }
</script>
